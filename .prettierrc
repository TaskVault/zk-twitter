{
  "trailingComma": "none",
  "singleQuote": false,
  "overrides": [
    {
      "files": "*.hbs",
      "options": {
        "parser": "html"
      }
    },
    {
      "files": "*.ts",
      "options": {
        "singleQuote": false
      }
    }
  ],
  "plugins": ["prettier-plugin-organize-imports"]
}
